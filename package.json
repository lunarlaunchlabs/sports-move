    "aptos": "^1.22.1",
    "dotenv": "^17.2.3",
    "next": "16.0.7",
    "react": "19.2.1",
    "react-confetti": "^6.4.0",
    "react-dom": "19.2.1",
    "react-icons": "^5.5.0",
    "recharts": "^3.5.1",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
