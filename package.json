    "contract:test": "cd move && movement move test",
    "contract:deploy": "cd move && movement move publish --assume-yes --included-artifacts none",
    "contract:init": "node scripts/initialize-contracts.js",
    "wallet:generate": "node scripts/generate-admin-wallets.js",
    "wallet:fund": "node scripts/fund-wallets.js",
    "wallet:register": "node scripts/register-remaining-admins.js",
    "wallet:pubkey": "node scripts/get-public-key.js",
    "test:contracts": "node test-sports-move-contracts.js",
    "test:contract-api": "node test-contract-api.js",
    "test:api-integration": "node test-api-integration.js",
    "setup:full": "npm run wallet:generate && npm run wallet:fund && npm run contract:compile && npm run contract:deploy && npm run contract:init",
    "setup:init-only": "npm run contract:init && npm run wallet:register"
  },
  "dependencies": {
    "@aptos-labs/wallet-adapter-core": "^7.8.0",
    "@aptos-labs/wallet-adapter-react": "^7.2.2",
